function diagonalDifference(arr) {
let c=0;
    let d=0;
    for(let i=0;i<arr.length;i++)
    {
        for(let j=0;j<arr.length;j++)
        {
            if(i==j)
            {
            c+=arr[i][j]}
            if(i+j==arr.length-1)
            d+=arr[i][j]
            
        }
    }
    console.log("c",c)
    console.log("d",d)
    let j=c-d;
return Math.abs(j)
}
