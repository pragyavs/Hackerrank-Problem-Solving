unction plusMinus(arr) {
    let countp=0;
    let countn=0;
    let countz=0;
    for(let i=0;i<arr.length;i++)
    {
        if(arr[i]<0){
        countn++;}
        else 
        if(arr[i]==0){
        countz++}
        else 
        if(arr[i]>0){
        countp++;}
    }
let length=arr.length;
let a=(countp/length).toFixed(6)
let b=(countn/length).toFixed(6)
let c=(countz/length).toFixed(6)
console.log(a)
console.log(b)
console.log(c)
}

